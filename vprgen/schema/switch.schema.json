{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "type": {
            "enum": ["mux", "tristate", "pass_gate", "short", "buffer"],
        },
        "name": {
            "type": "string",
        },
        "id": {
            "type": "integer",
            "minimum": 0,
        },
        "R": {
            "type": "number",
            "minimum": 0,
            "default": 0,
        },
        "Cin": {
            "type": "number",
            "minimum": 0,
            "default": 0,
        },
        "Cout": {
            "type": "number",
            "minimum": 0,
            "default": 0,
        },
        "Tdel": {
            "oneOf": [{
                "type": "number",
                "minimum": 0,
            }, {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "num_inputs": {
                            "type": "integer",
                            "exclusiveMinimum": 1,
                        },
                        "delay": {
                            "type": "number",
                            "minimum": 0,
                        },
                    },
                    "required": ["num_inputs", "delay"],
                },
            }],
        },
    },
    "required": ["type", "name", "id", "Tdel"],
}
