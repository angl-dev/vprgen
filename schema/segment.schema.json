{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$comment": "Bidirectional segments are not supported",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
        },
        "id": {
            "type": "integer",
            "exclusiveMinimum": 0,
        },
        "length": {
            "type": "integer",
            "exclusiveMinimum": 0,
        },
        "freq": {
            "type": "number",
            "minimum": 0,
            "default": 0,
        },
        "Rmetal": {
            "type": "number",
            "minimum": 0,
            "default": 0,
        },
        "Cmetal": {
            "type": "number",
            "minimum": 0,
            "default": 0,
        },
        "sb": {
            "type": "array",
            "items": {
                "type": "boolean",
            },
        },
        "cb": {
            "type": "array",
            "items": {
                "type": "boolean",
            },
        },
        "mux": {
            "type": "string",
        },
    },
    "required": ["name", "length", "id", "mux"], 
}
