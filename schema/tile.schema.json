{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
        },
        "block_type_id": {
            "type": "integer",
            "minimum": 0,
        },
        "x": {
            "type": "integer",
            "minimum": 0,
        },
        "y": {
            "type": "integer",
            "minimum": 0,
        },
        "xoffset": {
            "type": "integer",
            "minimum": 0,
            "default": 0,
        },
        "yoffset": {
            "type": "integer",
            "minimum": 0,
            "default": 0,
        },
    },
    "required": ["type", "block_type_id", "x", "y"],
}
